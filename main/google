#!/usr/bin/env bash

set -euo pipefail

#shellcheck disable=SC1091
source "$(dirname -- "${BASH_SOURCE[0]}")/../lib/functions.bash"
check_at_least_1_arg "$@"

log "Googling: $*"
search=""
for term; do
  search="${search}%20${term}"
done
readonly search
xdg-open "https://www.google.com/search?q=${search}" > /dev/null 2>&1 &
disown
